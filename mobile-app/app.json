{
  "expo": {
    "name": "Calculator",
    "slug": "calculator-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6a11cb"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.calculator.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6a11cb"
      },
      "package": "com.calculator.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "calculator"
  }
}